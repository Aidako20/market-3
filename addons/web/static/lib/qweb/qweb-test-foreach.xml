<templatesxml:space="preserve">
    <tt-name="iter-items">
        <tt-foreach="[3,2,1]"t-as="item">
[<tt-esc="item_index"/>:<tt-esc="item"/><tt-esc="item_value"/>]</t>
    </t>
    <resultid="iter-items">
[0:33]
[1:22]
[2:11]
    </result>

    <tt-name="iter-position">
        <tt-foreach="5"t-as="item">
-<tt-if="item_first">first</t><tt-if="item_last">last</t>(<tt-esc="item_parity"/>)</t>
    </t>
    <resultid="iter-position">
-first(even)
-(odd)
-(even)
-(odd)
-last(even)
    </result>

    <!--testintegerparam-->
    <tt-name="iter-int">
        <tt-foreach="3"t-as="item">
[<tt-esc="item_index"/>:<tt-esc="item"/><tt-esc="item_value"/>]</t>
    </t>
    <resultid="iter-int">
[0:00]
[1:11]
[2:22]
    </result>

    <!--testdictparam-->
    <tt-name="iter-dict">
        <tt-foreach="value"t-as="item">
[<tt-esc="item_index"/>:<tt-esc="item"/><tt-esc="item_value"/>-<tt-esc="item_parity"/>]</t>
    </t>
    <paramsid="iter-dict">{"value":{"a":1,"b":2,"c":3}}</params>
    <resultid="iter-dict">
[0:a1-even]
[1:b2-odd]
[2:c3-even]
    </result>
</templates>
