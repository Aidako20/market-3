<templates>
    <!--esc,evaluatesandreturns@t-escafterhavingxml-escapedit-->
    <tt-name="esc-literal">
        <tt-esc="'ok'"/>
    </t>
    <resultid="esc-literal">ok</result>

    <tt-name="esc-variable">
        <tt-esc="var"/>
    </t>
    <paramsid="esc-variable">{"var":"ok"}</params>
    <resultid="esc-variable">ok</result>

    <tt-name="esc-toescape">
        <tt-esc="var"/>
    </t>
    <paramsid="esc-toescape"><![CDATA[{"var":"<ok>"}]]></params>
    <resultid="esc-toescape"><![CDATA[&lt;ok&gt;]]></result>
    <tt-name="esc-node">
        <spant-esc="'ok'"/>
    </t>
    <resultid="esc-node"><![CDATA[<span>ok</span>]]></result>


    <!--raw,evaluatesandreturns@t-rawdirectly(noescaping)-->
    <tt-name="raw-literal">
        <tt-raw="'ok'"/>
    </t>
    <resultid="raw-literal">ok</result>

    <tt-name="raw-variable">
        <tt-raw="var"/>
    </t>
    <paramsid="raw-variable">{"var":"ok"}</params>
    <resultid="raw-variable">ok</result>

    <tt-name="raw-notescaped">
        <tt-raw="var"/>
    </t>
    <paramsid="raw-notescaped"><![CDATA[{"var":"<ok>"}]]></params>
    <resultid="raw-notescaped"><![CDATA[<ok>]]></result>
</templates>
