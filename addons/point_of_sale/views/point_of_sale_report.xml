        <fieldname="name">UserLabels</field>
        <fieldname="model">res.users</field>
        <fieldname="report_type">qweb-pdf</field>
        <fieldname="report_name">point_of_sale.report_userlabel</field>
        <fieldname="report_file">point_of_sale.report_userlabel</field>
        <fieldname="binding_model_id"ref="base.model_res_users"/>
