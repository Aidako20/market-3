<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="ReprintReceiptScreen"owl="1">
        <divclass="receipt-screenscreen">
            <divclass="screen-content">
                <divclass="top-content">
                    <spanclass="buttonback"t-on-click="confirm">
                        <iclass="fafa-angle-double-left"></i>
                        <span></span>
                        <span>Back</span>
                    </span>
                </div>
                <divclass="centered-content">
                    <divclass="buttonprint"t-on-click="tryReprint">
                        <iclass="fafa-print"></i>PrintReceipt
                    </div>
                    <divclass="pos-receipt-container">
                        <OrderReceiptorder="props.order"t-ref="order-receipt"/>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>
