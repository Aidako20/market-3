<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="PSNumpadInputButton"owl="1">
        <buttont-attf-class="{{_class}}"
                t-on-click="trigger('input-from-numpad',{key:props.value})">
            <tt-slot="default">
                <tt-if="props.text">
                    <tt-esc="props.text"/>
                </t>
                <tt-else="">
                    <tt-esc="props.value"/>
                </t>
            </t>
        </button>
    </t>

</templates>
