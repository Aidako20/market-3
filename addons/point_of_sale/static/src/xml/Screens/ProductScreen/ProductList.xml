<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="ProductList"owl="1">
        <divclass="product-list-container">
            <divt-if="props.products.length!=0"class="product-list">
                <tt-foreach="props.products"t-as="product"t-key="product.id">
                    <ProductItemproduct="product"/>
                </t>
            </div>
            <divt-else=""class="product-list-empty">
                <divclass="product-list-empty">
                    <tt-if="props.searchWord!==''">
                        <p>
                            Noresultsfoundfor"
                            <bt-esc="props.searchWord"></b>
                            ".
                        </p>
                    </t>
                    <tt-else="">
                        <p>Therearenoproductsinthiscategory.</p>
                    </t>
                </div>
            </div>
        </div>
    </t>

</templates>
