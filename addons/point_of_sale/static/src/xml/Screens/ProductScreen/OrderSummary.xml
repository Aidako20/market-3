<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="OrderSummary"owl="1">
        <divclass="summaryclearfix">
            <divclass="line">
                <divclass="entrytotal">
                    <spanclass="badge">Total:</span>
                    <spanclass="value">
                        <tt-esc="props.total"/>
                    </span>
                    <divt-if="props.tax"class="subentry">
                        Taxes:
                        <spanclass="value">
                            <tt-esc="props.tax"/>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>