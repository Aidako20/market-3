<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="CategoryBreadcrumb"owl="1">
        <spanclass="breadcrumb">
            <imgsrc="/point_of_sale/static/src/img/bc-arrow-big.png"class="breadcrumb-arrow"
                 alt="Slash"/>
            <spanclass="breadcrumb-button"
                  t-on-click="trigger('switch-category',props.category.id)">
                <tt-esc="props.category.name"></t>
            </span>
        </span>
    </t>

</templates>
