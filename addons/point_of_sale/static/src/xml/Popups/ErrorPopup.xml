<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="ErrorPopup"owl="1">
        <divrole="dialog"class="modal-dialog">
            <divclass="popuppopup-error">
                <pclass="title">
                    <tt-esc="props.title"/>
                </p>
                <pclass="body">
                    <tt-esc="props.body"/>
                </p>
                <divclass="footer">
                    <divclass="buttoncancel"t-on-click="confirm">
                        <tt-esc="props.confirmText"/>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>
