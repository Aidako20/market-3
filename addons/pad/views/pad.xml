    <templateid="assets_backend"name="padassets"inherit_id="web.assets_backend">
        <xpathexpr="."position="inside">
            <linkrel="stylesheet"href="/pad/static/src/css/etherpad.css"/>
            <scripttype="text/javascript"src="/pad/static/src/js/pad.js"/>
        </xpath>
    </template>

    <templateid="qunit_suite"name="padtests"inherit_id="web.qunit_suite_tests">
        <xpathexpr="//script[last()]"position="after">
            <scripttype="text/javascript"src="/pad/static/tests/pad_tests.js"></script>
        </xpath>
    </template>
</flectra>
