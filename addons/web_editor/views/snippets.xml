<!--options(data-selector,data-drop-in,data-drop-near,data-jstolinkjsobject)-->
<templateid="snippet_options">
    <!--t-field-->
    <divdata-js='many2one'
         data-selector="[data-oe-many2one-model]:not([data-oe-readonly])"
         data-no-check="true"/>

    <divdata-selector=".s_hr"
         data-drop-near="p,h1,h2,h3,blockquote,.s_hr"/>

    <divdata-js="VersionControl"
         data-selector="[data-snippet]"/>

    <divdata-js="ImageOptimize"
         data-selector="img">
        <tt-call="web_editor.snippet_options_image_optimization_widgets"/>
    </div>

    <divdata-js="DynamicSvg"data-selector="img">
        <we-colorpickerstring="Dynamiccolor"data-color="true"/>
    </div>
</template>

<!--defaultblock-->
<templateid="s_hr"name="Separator">
    <divclass="s_hrpt32pb32">
        <hrclass="s_hr_1pxs_hr_solidw-100mx-auto"/>
    </div>
</template>

