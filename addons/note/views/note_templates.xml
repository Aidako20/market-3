<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <templateid="assets_backend"inherit_id="web.assets_backend">
        <xpathexpr="//link[last()]"position="after">
            <linkrel="stylesheet"type="text/scss"href="/note/static/src/scss/note.scss"/>
        </xpath>
        <xpathexpr="//script[last()]"position="after">
             <scripttype="text/javascript"src="/note/static/src/js/systray_activity_menu.js"></script>
        </xpath>
    </template>

    <templateid="qunit_suite"name="note_tests"inherit_id="web.qunit_suite_tests">
        <xpathexpr="."position="inside">
            <scripttype="text/javascript"src="/note/static/tests/systray_activity_menu_tests.js"></script>
        </xpath>
    </template>
</flectra>
