<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <datanoupdate="1">
        <recordid="team_sales_department"model="crm.team">
            <fieldname="name">Sales</field>
            <fieldname="sequence">0</field>
            <fieldname="member_ids"eval="[(4,ref('base.user_admin'))]"/>
            <fieldname="company_id"eval="False"/>
        </record>

        <recordid="salesteam_website_sales"model="crm.team">
            <fieldname="name">Website</field>
            <fieldname="company_id"eval="False"/>
            <fieldname="active"eval="False"/>
        </record>

        <recordid="pos_sales_team"model="crm.team">
            <fieldname="name">PointofSale</field>
            <fieldname="company_id"eval="False"/>
            <fieldname="active"eval="False"/>
        </record>

        <recordid="ebay_sales_team"model="crm.team">
            <fieldname="name">eBay</field>
            <fieldname="active"eval="False"/>
        </record>

        <functionmodel="crm.team"name="message_unsubscribe"
            eval="[
            ref('sales_team.team_sales_department'),
            ref('sales_team.salesteam_website_sales'),
            ref('sales_team.ebay_sales_team'),
            ref('sales_team.pos_sales_team')],
            [ref('base.partner_root')]"/>
    </data>
</flectra>
