<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordid="partner_demo_company_pl"model="res.partner">
        <fieldname="name">PLCompany</field>
        <fieldname="vat">PL5795955811</field>
        <fieldname="street"></field>
        <fieldname="city">Racib√≥rz</field>
        <fieldname="country_id"ref="base.pl"/>
        
        <fieldname="zip">47-400</field>
        <fieldname="phone">+48512345678</field>
        <fieldname="email">info@company.plexample.com</field>
        <fieldname="website">www.plexample.com</field>
    </record>

    <recordid="demo_company_pl"model="res.company">
        <fieldname="name">PLCompany</field>
        <fieldname="partner_id"ref="partner_demo_company_pl"/>
    </record>

    <functionmodel="res.company"name="_onchange_country_id">
        <valueeval="[ref('demo_company_pl')]"/>
