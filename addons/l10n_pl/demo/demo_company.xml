    </function>

    <functionmodel="account.chart.template"name="try_loading">
        <valueeval="[ref('l10n_pl.pl_chart_template')]"/>
        <valuemodel="res.company"eval="obj().env.ref('l10n_pl.demo_company_pl')"/>
    </function>
</flectra>
