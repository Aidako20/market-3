<?xmlversion="1.0"encoding="UTF-8"?>
<templatesxml:space="preserve">
    <tt-name="mail.AttachmentDeleteConfirmDialog"owl="1">
        <DialogcontentClass="'o_AttachmentDeleteConfirmDialog'"title="getTitle()"size="'medium'"t-ref="dialog">
            <pclass="o_AttachmentDeleteConfirmDialog_mainText"t-esc="getBody()"/>
            <tt-set-slot="buttons">
                <buttonclass="o_AttachmentDeleteConfirmDialog_confirmButtonbtnbtn-primary"t-on-click="_onClickOk">Ok</button>
                <buttonclass="o_AttachmentDeleteConfirmDialog_cancelButtonbtnbtn-secondary"t-on-click="_onClickCancel">Cancel</button>
            </t>
        </Dialog>
    </t>
</templates>
