<?xmlversion="1.0"encoding="UTF-8"?>
<templatesxml:space="preserve">

    <tt-name="mail.DialogManager"owl="1">
        <divclass="o_DialogManager">
            <tt-if="env.messaging">
                <tt-foreach="env.messaging.dialogManager.dialogs"t-as="dialog"t-key="dialog.localId">
                    <Dialog
                        class="o_DialogManager_dialog"
                        dialogLocalId="dialog.localId"
                    />
                </t>
            </t>
        </div>
    </t>

</templates>
