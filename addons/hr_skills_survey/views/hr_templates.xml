<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <templateid="assets_backend"name="hr_skills_survey_assets_backend"inherit_id="web.assets_backend">
        <xpathexpr="."position="inside">
            <linkrel="stylesheet"type="text/scss"href="/hr_skills_survey/static/src/scss/hr_skills.scss"/>
        </xpath>
    </template>

    <recordid="resume_survey_line_view_form"model="ir.ui.view">
        <fieldname="name">hr.resume.line.form</field>
        <fieldname="model">hr.resume.line</field>
        <fieldname="inherit_id"ref="hr_skills.resume_line_view_form"/>
        <fieldeval="20"name="priority"/>
        <fieldname="arch"type="xml">
            <xpathexpr="//field[@name='line_type_id']"position="after">
                <fieldname="survey_id"attrs="{'invisible':[('display_type','!=','certification')]}"/>
            </xpath>
        </field>
    </record>
</flectra>
