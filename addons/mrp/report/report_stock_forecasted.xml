<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <templateid="mrp_report_product_product_replenishment"inherit_id="stock.report_product_product_replenishment">
        <xpathexpr="//tr[@name='draft_picking_in']"position="after">
            <trt-if="docs['draft_production_qty']['in']"name="draft_mo_in">
                <tdcolspan="2">ProductionofDraftMO</td>
                <tdt-esc="docs['draft_production_qty']['in']"class="text-right"/>
                <tdt-esc="docs['uom']"groups="uom.group_uom"/>
            </tr>
        </xpath>
        <xpathexpr="//tr[@name='draft_picking_out']"position="after">
            <trt-if="docs['draft_production_qty']['out']"name="draft_mo_out">
                <tdcolspan="2">ComponentofDraftMO</td>
                <tdt-esc="-docs['draft_production_qty']['out']"class="text-right"/>
                <tdt-esc="docs['uom']"groups="uom.group_uom"/>
            </tr>
        </xpath>
    </template>
</flectra>
