<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordid="payment_acquirer_form_inherit_payment_payulatam"model="ir.ui.view">
        <fieldname="name">payment.acquirer.form.inherit.payment.payulatam</field>
        <fieldname="model">payment.acquirer</field>
        <fieldname="inherit_id"ref="payment.acquirer_form"/>
        <fieldname="arch"type="xml">
            <xpathexpr='//group[@name="acquirer"]'position='inside'>
                <groupattrs="{'invisible':[('provider','!=','payulatam')]}">
                    <fieldname="payulatam_account_id"options="{'no_create':True}"/>
                    <fieldname="payulatam_merchant_id"/>
                    <fieldname="payulatam_api_key"/>
                </group>
            </xpath>
        </field>
    </record>
</flectra>
