<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordid="partner_demo_company_si"model="res.partner">
        <fieldname="name">SICompany</field>
        <fieldname="vat">SI33331464</field>
        <fieldname="street">39Jamovacesta</field>
        <fieldname="city">Ljubljana</field>
        <fieldname="country_id"ref="base.si"/>
        
        <fieldname="zip">1102</field>
        <fieldname="phone">+38631234567</field>
        <fieldname="email">info@company.siexample.com</field>
        <fieldname="website">www.siexample.com</field>
    </record>

    <recordid="demo_company_si"model="res.company">
        <fieldname="name">SICompany</field>
        <fieldname="partner_id"ref="partner_demo_company_si"/>
    </record>

    <functionmodel="res.company"name="_onchange_country_id">
        <valueeval="[ref('demo_company_si')]"/>
    </function>

    <functionmodel="res.users"name="write">
        <valueeval="[ref('base.user_root'),ref('base.user_admin'),ref('base.user_demo')]"/>
        <valueeval="{'company_ids':[(4,ref('l10n_si.demo_company_si'))]}"/>
    </function>

    <functionmodel="account.chart.template"name="try_loading">
        <valueeval="[ref('l10n_si.gd_chart')]"/>
        <valuemodel="res.company"eval="obj().env.ref('l10n_si.demo_company_si')"/>
    </function>
</flectra>
