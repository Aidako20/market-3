<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordid="sale_coupon_view_tree"model="ir.ui.view">
        <fieldname="name">coupon.coupon.tree</field>
        <fieldname="model">coupon.coupon</field>
        <fieldname="inherit_id"ref="coupon.coupon_view_tree"/>
        <fieldname="arch"type="xml">
            <xpathexpr="//field[@name='partner_id']"position="after">
                <fieldname="order_id"/>
            </xpath>
        </field>
    </record>

    <recordid="sale_coupon_view_form"model="ir.ui.view">
        <fieldname="name">coupon.coupon.form</field>
        <fieldname="model">coupon.coupon</field>
        <fieldname="inherit_id"ref="coupon.coupon_view_form"/>
        <fieldname="arch"type="xml">
            <xpathexpr="//field[@name='partner_id']"position="after">
                <fieldname="order_id"/>
                <fieldname="sales_order_id"/>
            </xpath>
        </field>
    </record>

</flectra>
