<flectra>
    <!--PAYMENTACQUIRER-->
    <templateid="website_sale_onboarding_payment_acquirer_step"primary="True"
              inherit_id="payment.onboarding_payment_acquirer_step">
        <xpathexpr="//t[@t-set='method']"position="replace">
            <tt-set="method"t-value="'action_open_website_sale_onboarding_payment_acquirer'"/>
        </xpath>
        <xpathexpr="//t[@t-set='state']"position="replace">
            <tt-set="state"t-value="state.get('website_sale_onboarding_payment_acquirer_state')"/>
        </xpath>
    </template>

    <recordid="action_open_website_sale_onboarding_payment_acquirer_wizard"model="ir.actions.act_window">
        <fieldname="name">Chooseapaymentmethod</field>
        <fieldname="type">ir.actions.act_window</field>
        <fieldname="res_model">website.sale.payment.acquirer.onboarding.wizard</field>
        <fieldname="view_mode">form</field>
        <fieldname="view_id"ref="payment.payment_acquirer_onboarding_wizard_form"/>
        <fieldname="target">new</field>
    </record>
</flectra>
