<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
<templateid="report_producttemplatelabel">
    <tt-call="web.basic_layout">
        <divclass="page">
            <tt-foreach="docs"t-as="template">
                <tt-foreach="template.product_variant_ids"t-as="product">
                    <tt-call="product.report_simple_label">
                        <tt-set="product"t-value="product"/>
                    </t>
                </t>
            </t>
        </div>
    </t>
</template>
<templateid="report_producttemplatebarcode">
    <tt-call="web.basic_layout">
        <divclass="page">
            <tt-foreach="docs"t-as="template">
                <tt-foreach="template.product_variant_ids"t-as="product">
                    <tt-call="product.report_simple_barcode">
                        <tt-set="product"t-value="product"/>
                    </t>
                </t>
            </t>
        </div>
    </t>
</template>
</flectra>
