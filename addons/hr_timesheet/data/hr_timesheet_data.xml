<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <!--SettheJSwidget-->
    <recordid="uom.product_uom_day"model="uom.uom">
        <fieldname="timesheet_widget">float_toggle</field>
    </record>

    <recordid="uom.product_uom_hour"model="uom.uom">
        <fieldname="timesheet_widget">float_time</field>
    </record>

    <!--ForceAnalyticaccountcreationforprojectsallowingtimesheet(defaultisTrue)-->
    <function
        model="project.project"
        name="_init_data_analytic_account"
        eval="[]"/>

    <datanoupdate="1">
        <recordid="ir_config_parameter_timesheet_rounding"model="ir.config_parameter">
            <fieldname="key">hr_timesheet.timesheet_rounding</field>
            <fieldname="value">15</field>
        </record>
        <recordid="ir_config_parameter_timesheet_min_duration"model="ir.config_parameter">
            <fieldname="key">hr_timesheet.timesheet_min_duration</field>
            <fieldname="value">15</field>
        </record>
    </data>

</flectra>
