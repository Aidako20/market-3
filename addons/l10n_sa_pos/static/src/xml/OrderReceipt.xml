<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">
    <tt-inherit="point_of_sale.OrderReceipt"t-inherit-mode="extension"owl="1">
        <xpathexpr="//img[hasclass('pos-receipt-logo')]"position="after">
            <tif="receipt.is_gcc_country">
                <imgt-if="receipt.qr_code"id="qrcode"t-att-src="receipt.qr_code"class="pos-receipt-logo"/>
                <br/>
            </t>
        </xpath>

        <xpathexpr="//span[@id='title_english']"position="replace">
            <spanid="title_english">SimplifiedTaxInvoice</span>
        </xpath>

        <xpathexpr="//span[@id='title_arabic']"position="replace">
            <spanid="title_arabic">فاتورةضريبيةمبسطة</span>
        </xpath>
    </t>
</templates>
