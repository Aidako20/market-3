<?xmlversion="1.0"encoding="UTF-8"?>
<templatesid="template"xml:space="preserve">

    <tt-name="BillScreen"owl="1">
        <divclass="receipt-screenscreen">
            <divclass="screen-content">
                <divclass="top-content">
                    <spanclass="buttonback"t-on-click="confirm">
                        <iclass="fafa-angle-double-left"></i>
                        <span></span>
                        <span>Back</span>
                    </span>
                    <divclass="top-content-center">
                        <h1>BillPrinting</h1>
                    </div>
                    <spanclass="buttonnexthighlight"t-on-click="confirm">
                        <span>Ok</span>
                        <span></span>
                        <iclass="fafa-angle-double-right"></i>
                    </span>
                </div>
                <divclass="centered-content">
                    <divclass="buttonprint"t-on-click="printReceipt">
                        <iclass="fafa-print"></i>
                        <span></span>
                        <span>Print</span>
                    </div>
                    <divclass="pos-receipt-container">
                        <OrderReceiptorder="currentOrder"isBill="true"t-ref="order-receipt"/>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>
