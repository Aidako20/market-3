<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
<templateid="report_accounttest">
    <tt-call="web.html_container">
        <tt-call="web.internal_layout">
            <divclass="page">
                <h2>Accountingtestson<spant-esc="datetime.datetime.now().strftime('%Y-%m-%d%H:%M:%S')"/></h2>
                <divt-foreach="docs"t-as="o">
                    <p>
                        <strong>Name:</strong><spant-field="o.name"/><br/>
                        <strong>Description:</strong><spant-field="o.desc"/>
                    </p>
                    <pt-foreach="execute_code(o.code_exec)"t-as="test_result">
                        <spant-esc="test_result"/>
                    </p>
                </div>
            </div>
        </t>
    </t>
</template>
</flectra>
