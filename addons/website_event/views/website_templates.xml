<?xmlversion="1.0"encoding="utf-8"?>
<flectra>

<!--UserNavbar-->
<templateid="user_navbar_inherit_website_event"inherit_id="website.user_navbar">
    <xpathexpr="//div[@id='o_new_content_menu_choices']//div[@name='module_website_event']"position="attributes">
        <attributename="name"/>
        <attributename="t-att-data-module-id"/>
        <attributename="t-att-data-module-shortdesc"/>
        <attributename="groups">event.group_event_manager</attribute>
    </xpath>
</template>

<!--UserNavbar-EditOptions-->
<templateid="event_edit_options"inherit_id="website.user_navbar"name="EditEventOptions">
    <xpathexpr="//li[@id='edit-page-menu']"position="after">
        <tt-if="main_object._name=='event.event'"t-set="action"t-value="'event.action_event_view'"/>
    </xpath>
</template>

</flectra>
