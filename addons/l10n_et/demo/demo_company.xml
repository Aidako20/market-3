<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordid="partner_demo_company_et"model="res.partner">
        <fieldname="name">ETCompany</field>
        <fieldname="vat"></field>
        <fieldname="street"></field>
        <fieldname="city">DeboDibo</field>
        <fieldname="country_id"ref="base.et"/>
        <fieldname="state_id"ref="base.state_et_11"/>
        <fieldname="zip"></field>
        <fieldname="phone">+251911234567</field>
        <fieldname="email">info@company.etexample.com</field>
        <fieldname="website">www.etexample.com</field>
    </record>

    <recordid="demo_company_et"model="res.company">
        <fieldname="name">ETCompany</field>
        <fieldname="partner_id"ref="partner_demo_company_et"/>
    </record>

    <functionmodel="res.company"name="_onchange_country_id">
        <valueeval="[ref('demo_company_et')]"/>
    </function>

    <functionmodel="res.users"name="write">
        <valueeval="[ref('base.user_root'),ref('base.user_admin'),ref('base.user_demo')]"/>
        <valueeval="{'company_ids':[(4,ref('l10n_et.demo_company_et'))]}"/>
    </function>

    <functionmodel="account.chart.template"name="try_loading">
        <valueeval="[ref('l10n_et.l10n_et')]"/>
        <valuemodel="res.company"eval="obj().env.ref('l10n_et.demo_company_et')"/>
    </function>
</flectra>
