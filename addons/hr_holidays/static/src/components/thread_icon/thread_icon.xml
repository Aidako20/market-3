<?xmlversion="1.0"encoding="UTF-8"?>
<templatesxml:space="preserve">
    <tt-inherit="mail.ThreadIcon"t-inherit-mode="extension">
        <xpathexpr="//*[@name='noImStatusCondition']"position="before">
            <tt-elif="thread.correspondent.im_status==='leave_online'">
                <divclass="o_ThreadIcon_leaveOnlinefafa-plane"title="Online"/>
            </t>
            <tt-elif="thread.correspondent.im_status==='leave_offline'">
                <divclass="o_ThreadIcon_leaveOfflinefafa-plane"title="Outofoffice"/>
            </t>
        </xpath>
    </t>
</templates>
