<?xmlversion="1.0"encoding="utf-8"?>
<flectra>

    <templateid="track_po_line_template">
        <div>
            <strong>Theorderedquantityhasbeenupdated.</strong>
            <ul>
                <li><tt-esc="line.product_id.display_name"/>:</li>
                OrderedQuantity:<tt-esc="line.product_qty"/>-&gt;<tt-esc="float(product_qty)"/><br/>
                <tt-if='line.order_id.product_id.typein("consu","product")'>
                    ReceivedQuantity:<tt-esc="line.qty_received"/><br/>
                </t>
                BilledQuantity:<tt-esc="line.qty_invoiced"/>
            </ul>
        </div>
    </template>

    <templateid="track_po_line_qty_received_template">
        <div>
            <strong>Thereceivedquantityhasbeenupdated.</strong>
            <ul>
                <li><tt-esc="line.product_id.display_name"/>:</li>
                ReceivedQuantity:<tt-esc="line.qty_received"/>-&gt;<tt-esc="float(qty_received)"/><br/>
            </ul>
        </div>
    </template>

</flectra>
