<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
        <recordid="view_users_form"model="ir.ui.view">
            <fieldname="name">res.users.form.inherit</field>
            <fieldname="model">res.users</field>
            <fieldname="type">form</field>
            <fieldname="inherit_id"ref="base.view_users_form"/>
            <fieldname="arch"type="xml">
                <xpathexpr="//page[@name='access_rights']"position="after">
                    <pagestring="Oauth"name="oauth">
                        <group>
                            <fieldname="oauth_provider_id"/>
                            <fieldname="oauth_uid"/>
                            <fieldname="oauth_access_token"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
</flectra>
