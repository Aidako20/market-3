<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <data>
        <recordid="account_move_form_l10n_cn"model="ir.ui.view">
            <fieldname="name">l10n_cn.account.move.form</field>
            <fieldname="model">account.move</field>
            <fieldname="inherit_id"ref="account.view_move_form"/>
            <fieldname="arch"type="xml">
                <xpathexpr="//field[@name='ref']"position="after">
                    <fieldname="move_type"invisible='1'/>
                    <fieldname="fapiao"attrs="{'invisible':['|',('country_code','!=','CN'),
                        ('move_type','notin',['out_invoice','out_refund','in_invoice','in_refund'])]}"/>
                </xpath>
            </field>
        </record>
    </data>
</flectra>
