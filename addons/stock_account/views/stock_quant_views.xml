<?xmlversion="1.0"encoding="utf-8"?>
<flectra>
    <recordmodel="ir.ui.view"id="view_stock_quant_tree_inherit">
        <fieldname="name">stock.quant.tree.inherit</field>
        <fieldname="model">stock.quant</field>
        <fieldname="inherit_id"ref="stock.view_stock_quant_tree"></field>
        <fieldname="arch"type="xml">
            <xpathexpr="//field[@name='product_uom_id']"position="after">
                <fieldname="currency_id"invisible="1"/>
                <fieldname="value"/>
            </xpath>
        </field>
    </record>

    <recordmodel="ir.ui.view"id="view_stock_quant_tree_editable_inherit">
        <fieldname="name">stock.quant.tree.editable.inherit</field>
        <fieldname="model">stock.quant</field>
        <fieldname="inherit_id"ref="stock.view_stock_quant_tree_editable"></field>
        <fieldname="arch"type="xml">
            <xpathexpr="//field[@name='product_uom_id']"position="after">
                <fieldname="currency_id"invisible="1"/>
                <fieldname="value"/>
            </xpath>
        </field>
    </record>

</flectra>
